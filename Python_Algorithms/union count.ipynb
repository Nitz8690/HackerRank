    "def unionCount(a,b,c):\n",
    "    d = a + b + c\n",
    "    f=[]\n",
    "    c = 0\n",
    "    for i in d:\n",
    "        if not i in f:\n",
    "            f.append(i)\n",
    "            c += 1\n",
    "    return c"
   ]
